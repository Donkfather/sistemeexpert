reg ( 34 )
tip_de_comunicare ~ prin_fum ~ valoare factor cert ~ 99
enumerare_premise ( 
     in_viata +
     locatie ~ insula +
     distanta ~ medie +
     not(semnal_telefon) +
     lemne +
     d_orb ~ nu +
     e_orb ~ nu
).

